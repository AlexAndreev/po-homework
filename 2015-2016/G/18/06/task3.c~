#include <stdio.h>
#include <string.h>

int create_letter_distribution_diagram(char text[300], int dia[26][2]);
int main()

{
	char text[300];
	int dia[26][2];
	scanf("%[^\n]s", text);
	create_letter_distribution_diagram(text, dia);
	return 0;
}

int create_letter_distribution_diagram(char text[300], int dia[26] [2])
{
	int letter;
	for(letter = 0; letter < 26; letter++)
		{
			dia[letter][0] == 'a' + letter;	
		}
	for(letter = 0; letter < 26; letter++)
		{
			printf("%s", dia[letter][0]);
		}
	return 0;
}
