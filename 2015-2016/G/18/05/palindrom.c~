#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main()
{
    int l,i,ok,k;
    char s[100];
    scanf("%s",s);
    l=strlen(s)/2;
k=strlen(s);
    for(i=0;i<l;i++)
    {
        if(s[i]!=s[k-i-1]){ok=2; break;}
        else ok=1;
    }
    if(ok==2) printf("\nNot\n");
    if(ok==1) printf("\nPolindrom\n");
return 0;
}

